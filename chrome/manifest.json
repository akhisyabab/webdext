{
  "manifest_version": 2,
  "name": "Webdext",
  "description": "Intelligent Web Data Extractor",
  "version": "0.0.1",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Webdext",
    "default_popup": "popup.html"
  }
}
